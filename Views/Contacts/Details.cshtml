                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <hr class="hrFieldBufferWithinPanel" />
                                            </div>
                                        </div>

                                        @{
                                            foreach (var client in Model.clients)
                                            {

                                                <div class="row">
                                                    <div class="col-sm-4" style="padding:0px;">
